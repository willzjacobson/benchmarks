<!DOCTYPE html>
<html>
<head>  
      <meta charset="utf-8">
        <title>D3 Test</title>
        <script type="text/javascript" src="{{STATIC_URL}}d3/d3/d3.v3.js"></script>
 </head>
	<body>
		Test Page
	<img src="{{STATIC_URL}}/pulpit.jpg" alt="No image">
		<table border=1>
			{% for item in dates %}
			<tr><td>{{item}}</td></tr>
			{% endfor %}
		</script>
		</table>
<script type="text/javascript">
	d3.select("body").append("p").text("New Para");
</script>
<table border=3>
	<tr><td>Celsius</td><td>Farenheit</td></tr>
<script type="text/javascript">
	dataList = new Array;
	for(i=0;i<51;i++){
		dataList[i] = (i*1.8)+32;
	}
	var j=0;
	{% for item in dates %}
			dataList[j] = {{item}};
			j += 1;
			{% endfor %}
	for(i=0;i<51;i++){
		document.write("<tr><td>",i,"</td><td>",dataList[i],"</td></tr>");
	}
			
</script>
	</body>
</html>
