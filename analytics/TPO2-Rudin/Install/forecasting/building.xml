	<key name="building_db"></key>
	<key name="building_db_server"></key>

	<key name="bms">
		<optional>True</optional>
	</key>
	
	<key name="db_user"></key>
	<key name="db_pwd"></key>
	
	<key name="building_floors">
		<source>pointslist</source>
		<sig_code>\w{3}\w{3}(F\w{2})(C\w{2}|\-{3})\d{3}TPO(HVA|FOR)TEMSPA\d{3}</sig_code>
		<value_group>1</value_group>
		<mode>list</mode>
	</key>

	<key name="floor_quadrants">
		<source>pointslist</source>
		<sig_code>\w{3}\w{3}F\w{2}(C\w{2}|\-{3})\d{3}TPO(HVA|FOR)TEMSPA\d{3}</sig_code>
		<value_group>1</value_group>
		<mode>list</mode>
	</key>
	
	<key name="zones">
		<source>pointslist</source>
		<sig_code>\w{3}(\w{3})F\w{2}(C\w{2}|\-{3})\d{3}TPO(HVA|FOR)TEMSPA\d{3}</sig_code>
		<value_group>1</value_group>
		<mode>list</mode>
	</key>

	<key name="sign_prog_space_temp">
		<source>assign</source>
		<start_num>1</start_num>
		<count_match_key>building_floors</count_match_key>
	</key>

